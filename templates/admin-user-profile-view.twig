{% extends 'index-layout.twig' %}
{% block content %}

<div id="main-container">

<ul class="tab-bar grey-tab">
    <li class="active">
        <a href="#overview" data-toggle="tab">
						<span class="block text-center">
							<i class="fa fa-home fa-2x"></i>
						</span>
            Overview
        </a>
    </li>
    <li>
        <a href="#edit" data-toggle="tab">
						<span class="block text-center">
							<i class="fa fa-edit fa-2x"></i>
						</span>
            Edit Profile
        </a>
    </li>

</ul>

<div class="padding-md">
<div class="row">
<div class="col-md-3 col-sm-3">
    <div class="row">
        <div class="col-xs-6 col-sm-12 col-md-6 text-center">
            {% if profile.ProfileImage == ''  %}
                <img id="profile-image" src="/assets/img/default-profile.jpg" alt="User Avatar" class="img-thumbnail" >
            {% else %}
                <img id="profile-image" src="/{{ profile.ProfileImage }}" alt="User Avatar" class="img-thumbnail">
            {% endif %}

            <div class="seperator"></div>
            <div class="seperator"></div>

        </div><!-- /.col -->
        <div class="col-xs-6 col-sm-12 col-md-6">
            <strong class="font-14">{{ user.UserName | capitalize }}</strong>
            <small class="block text-muted">
                {{ user.EmailAddress }}
            </small>
            <div class="seperator"></div>
            <div class="seperator"></div>
            <div class="seperator"></div>

            <form action="" method="post" enctype="multipart/form-data">
                <input type="hidden" name="_METHOD" value="PUT"/>
                <input type="hidden" name="form-part" value="profile-image">
                <input type="file" name="file" >
                <div class="seperator"></div>
                <input type="submit" value="Upload image" >
            </form>

        </div><!-- /.col -->
    </div><!-- /.row -->

</div><!-- /.col -->
<div class="col-md-9 col-sm-9">
<div class="tab-content">
<div class="tab-pane fade in active" id="overview">

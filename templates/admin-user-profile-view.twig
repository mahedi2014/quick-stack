<form action="" method="post">
<input type="hidden" name="_METHOD" value="PUT"/>
<div class="col-md-6">
    <input type="hidden" name="form-part" value="profile">

    <div class="form-group  col-md-8">
        <label class="control-label">First name</label>
        <input type="text"  name="first-name" placeholder="First name" class="form-control input-sm parsley-validated"  value ="{% if profile.FirstName != '' %} {{ profile.FirstName | capitalize }} {% endif %}"  >
    </div>

    <div class="form-group  col-md-8">
        <label class="control-label">Last name</label>
        <input type="text" name="last-name" placeholder="Last name" class="form-control input-sm parsley-validated" value ="{% if profile.LastName != '' %} {{ profile.LastName | capitalize }} {% endif %}"  >
    </div>

    <div class="form-group  col-md-8">
        <label class="control-label">Gender</label>
        <select name="gender" class="form-control input-sm parsley-validated">
            <option value="">Select one</option>
            <option value="male" {% if  profile.Gender | capitalize == 'Male' %} selected="selected"{% endif %}>Male </option>
            <option value="female" {% if  profile.Gender | capitalize == 'Female' %} selected="selected"{% endif %}>Female</option>

        </select>
    </div>

    <div class="form-group  col-md-6">
        <label class="control-label">Date of birth</label>
        <div class="input-group">
            <input name="date-of-birth" type="text" data-date="2014-02-29" data-date-format="yyyy-mm-dd"   class="input-datepicker form-control" value="{{ profile.DateOfBirth | date('Y-m-d') }}">
            <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
        </div>
    </div>

    <div class="form-group  col-md-8">
        <label class="control-label">Email</label>
        <input type="text" name="email" disabled="disabled" placeholder="Email address" class="form-control input-sm parsley-validated" value="{{ user.EmailAddress }}" >
    </div>

    <div class="form-group  col-md-8">
        <label class="control-label">Phone number</label>
        <input type="text" name="phone-number" placeholder="Phone number" class="form-control input-sm parsley-validated" value="{{ profile.PhoneNumber }}" >
    </div>

    <div class="form-group  col-md-8">
        <label class="control-label">Mobile number</label>
        <input type="text" name="mobile-number" placeholder="Mobile number" class="form-control input-sm parsley-validated" value="{{ profile.MobileNumber }}" >
    </div>
</div> <!-- first col end-->

